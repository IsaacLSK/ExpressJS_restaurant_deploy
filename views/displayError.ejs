<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial=1.0">
        <link rel="stylesheet" href="../css/styles.css" charset="utf-8">
        <title>Error</title>
    </head>
<body>
    <h1>Error found !</h1>
    <hr>
    <% if(error){ %>
        <% error.forEach(function(error){ %>
        <div class="error"><%= error.msg %></div>
        <% }) %>
      <% } %>
      
    <form action="/">
        <input type="submit" value="Back">
    </form>
</body>
</html>
